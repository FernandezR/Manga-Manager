<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Frame" id="_frame_1_title">
    <property name="container">false</property>
    <property name="height">800</property>
    <property name="highlightbackground">red</property>
    <property name="highlightcolor">red</property>
    <property name="pady">20</property>
    <property name="width">1150</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="padx">25</property>
      <property name="pady">25</property>
      <property name="propagate">False</property>
      <property name="row">1</property>
      <property name="sticky">ew</property>
      <property type="col" id="0" name="pad">25</property>
      <property type="col" id="0" name="weight">1</property>
      <property type="row" id="1" name="minsize">0</property>
      <property type="row" id="1" name="pad">45</property>
      <property type="row" id="1" name="uniform" />
      <property type="row" id="1" name="weight">1</property>
    </layout>
    <child>
      <object class="tk.Frame" id="_settings">
        <property name="container">false</property>
        <property name="height">160</property>
        <property name="highlightbackground">black</property>
        <property name="highlightcolor">black</property>
        <property name="highlightthickness">01</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">0 70</property>
          <property name="pady">10 0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="rowspan">4</property>
          <property name="sticky">e</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="3" name="weight">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="_label_2_settings">
            <property name="anchor">center</property>
            <property name="font">TkMenuFont</property>
            <property name="text" translatable="yes">Settings</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
              <property type="col" id="0" name="weight">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Checkbutton" id="_checkbutton_1_settings">
            <property name="text" translatable="yes">Auto increase volume number after processing</property>
            <property name="variable">boolean:checkbutton_1_settings_val</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
              <property type="col" id="0" name="weight">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Checkbutton" id="_checkbutton_2_settings">
            <property name="text" translatable="yes">Open File selector dialog after processing</property>
            <property name="variable">boolean:checkbutton_2_settings_val</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">w</property>
              <property type="col" id="0" name="weight">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Checkbutton" id="_checkbutton_3_settings">
            <property name="text" translatable="yes">Automatic preview</property>
            <property name="variable">boolean:checkbutton_3_settings_val</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
              <property type="col" id="0" name="weight">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Checkbutton" id="_checkbutton_4_settings">
            <property name="text" translatable="yes">Add volume number to ComicInfo if it exists</property>
            <property name="variable">boolean:checkbutton_4_settings_val</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
              <property name="sticky">w</property>
              <property type="col" id="0" name="weight">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="_label_1">
        <property name="font">{Title} 20 {bold}</property>
        <property name="text" translatable="yes">Volume Manager</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="_label_2_instructions">
        <property name="font">{subheader} 12 {}</property>
        <property name="text" translatable="yes">This script will append Vol.XX just before any Ch X.ext / Chapter XX.ext</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="_label_3_subinstructions">
        <property name="text" translatable="yes">This naming convention must be followed for the script to work properly</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="_label_4_selected_files">
        <property name="textvariable">string:label_4_selected_files_val</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="3" name="weight">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="_button_1_openfiles">
        <property name="command" type="command" cbtype="simple">_open_files</property>
        <property name="text" translatable="yes">Open</property>
        <property name="width">15</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="_label_5_volume_number_input">
        <property name="text" translatable="yes">Volume number to apply to the selected files</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Spinbox" id="_spinbox_1_volume_number">
        <property name="from_">1</property>
        <property name="state">normal</property>
        <property name="textvariable">int:_spinbox_1_volume_number_val</property>
        <property name="to">500</property>
        <property name="validate">all</property>
        <property name="validatecommand" type="command" cbtype="entry_validate" args="">_validate_spinbox</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="_button_2_preview">
        <property name="command" type="command" cbtype="simple">_preview_changes</property>
        <property name="text" translatable="yes">Preview</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="7" name="pad">20</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Treeview" id="_treeview_1">
        <property name="height">16</property>
        <property name="padding">0 5</property>
        <property name="selectmode">browse</property>
        <property name="style">Toolbutton</property>
        <bind sequence="&lt;Button-1&gt;" handler="_handle_click" add="" />
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">8</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="8" name="pad">25</property>
        </layout>
        <child>
          <object class="ttk.Treeview.Column" id="column_1">
            <property name="column_anchor">w</property>
            <property name="heading_anchor">w</property>
            <property name="minwidth">0</property>
            <property name="stretch">true</property>
            <property name="tree_column">true</property>
            <property name="visible">true</property>
            <property name="width">0</property>
          </object>
        </child>
        <child>
          <object class="ttk.Treeview.Column" id="old_name">
            <property name="column_anchor">center</property>
            <property name="heading_anchor">center</property>
            <property name="minwidth">20</property>
            <property name="stretch">false</property>
            <property name="text" translatable="yes">OLD NAME</property>
            <property name="tree_column">false</property>
            <property name="visible">true</property>
            <property name="width">525</property>
          </object>
        </child>
        <child>
          <object class="ttk.Treeview.Column" id="to">
            <property name="column_anchor">center</property>
            <property name="heading_anchor">center</property>
            <property name="minwidth">26</property>
            <property name="stretch">false</property>
            <property name="text" translatable="yes">to</property>
            <property name="tree_column">false</property>
            <property name="visible">true</property>
            <property name="width">26</property>
          </object>
        </child>
        <child>
          <object class="ttk.Treeview.Column" id="new_name">
            <property name="column_anchor">center</property>
            <property name="heading_anchor">center</property>
            <property name="minwidth">20</property>
            <property name="stretch">true</property>
            <property name="text" translatable="yes">NEW NAME</property>
            <property name="tree_column">false</property>
            <property name="visible">true</property>
            <property name="width">525</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="_frame_2_finalButtons">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">9</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="9" name="pad">30</property>
        </layout>
        <child>
          <object class="tk.Button" id="_button_4_clearqueue">
            <property name="command" type="command" cbtype="simple">_clear_queue</property>
            <property name="font">{Custom} 11 {bold}</property>
            <property name="text" translatable="yes">Clear Queue</property>
            <property name="width">15</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="_button_3_proceed">
            <property name="command" type="command" cbtype="simple">_pre_process</property>
            <property name="font">{Custom} 11 {bold}</property>
            <property name="text" translatable="yes">Proceed</property>
            <property name="width">15</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property type="col" id="0" name="pad">15</property>
              <property type="col" id="0" name="weight">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame_1_progressbar">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">10</property>
          <property type="col" id="0" name="pad">15</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="10" name="pad">10</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
